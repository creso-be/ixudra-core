{
    "name": "ixudra/core",
    "description": "Custom PHP Core library for the Laravel 5 framework - developed by Ixudra",
    "version": "6.0.1",
    "keywords": ["Ixudra", "Laravel", "Core", "Project"],
    "homepage": "http://ixudra.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@gmail.com"
        }
    ],

    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0",
        "laracasts/presenter": "0.2.*"
    },

    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },

    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Ixudra\\Core\\": "src/"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Ixudra\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "Ixudra\\Core\\Core"
            }
        }
    }
}
